<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:drones="clr-namespace:Asv.Drones"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Asv.Drones.RenameDialogView"
             x:DataType="drones:RenameDialogViewModel">
    <StackPanel Spacing="10" Margin="10">
        <TextBlock Text="{x:Static drones:RS.FileBrowserViewModel_RenameDialog_Message}"/>
        <TextBox Text="{Binding NewName.Value}"/>
    </StackPanel>
</UserControl>
