<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:drones="clr-namespace:Asv.Drones"
             mc:Ignorable="d" d:DesignWidth="820" d:DesignHeight="20"
             x:Class="Asv.Drones.TryCloseWithApprovalDialogView"
             x:DataType="drones:TryCloseWithApprovalDialogViewModel">
    <Design.DataContext>
        <drones:TryCloseWithApprovalDialogViewModel />
    </Design.DataContext>
    <StackPanel>
        <TextBlock Text="{CompiledBinding Message, Mode=OneTime}"/>
    </StackPanel>
</UserControl>
